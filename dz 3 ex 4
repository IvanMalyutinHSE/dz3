    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "x = [-1, -0.7, -0.43, -0.14, -0.14, 0.43, 0.71, 1, 1.29, 1.57, 1.86, 2.14, 2.43, 2.71, 3]\n",
    "y = [-2.25, -0.77, 0.21, 0.44, 0.64, 0.03, -0.22, -0.84, -1.2, -1.03, -0.37, 0.61, 2.67, 5.04, 8.90]\n",
    "n = len(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def design_matrix(m = 5, x=x):\n",
    "    design_matrix = []\n",
    "    for i in range(len(x)): \n",
    "        row = []\n",
    "        for s in range(m + 1):\n",
    "            row.append(x[i]**s)\n",
    "        design_matrix.append(row)\n",
    "    D = np.array(design_matrix)\n",
    "    return D"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solve_sys(m=5, x=x):\n",
    "    D = design_matrix(m, x)\n",
    "    F = D.T @ D\n",
    "    u = D.T @ y\n",
    "    return D, np.linalg.solve(F, u)"
   ]
  },
