    "def sigma(m):\n",
    "    summ = 0 \n",
    "    solve = solve_sys(m)\n",
    "    answer = solve[0] @ solve[1]\n",
    "    summ = 1/(n - m)*sum((answer - y)**2)\n",
    "    return summ"
   ]
  },
